{
  "id": "020_project_analyzer",
  "request": {
    "text": "Analyze this repo: {{REPO_ROOT}}. Call project_analyzer first. Return a short summary."
  },
  "assert": {
    "status": 200,
    "tool_must_include": ["project_analyzer"],
    "tool_output_must_have": {
      "project_analyzer": ["entrypoint", "runtime_hints", "repo_path"]
    },
    "tool_output_path_must_exist": {
      "project_analyzer": ["runtime_hints.port", "runtime_hints.start_command"]
    },
    "must_not_contain": ["Traceback", "Internal Server Error"]
  }
}
